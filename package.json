{
  "name": "beatz-bot",
  "version": "0.1.0",
  "description": "Beatz Bot is BronyCon Slack assistant, at your service!",
  "main": "lib/index.js",
  "scripts": {
    "build": "npm-run-all clean test transpile",
    "clean": "rimraf lib",
    "coverage": "nyc mocha --require babel-core/register --recursive",
    "transpile": "babel src -d lib",
    "lint": "eslint src test",
    "preversion": "npm run build",
    "postversion": "git push && git push --tags",
    "start-dev": "npm run clean && npm run transpile && node lib/index.js",
    "test": "npm-run-all lint test:unit",
    "test:unit": "mocha --require babel-core/register --recursive"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mjstahl/es6-starter-kit.git"
  },
  "author": "Josh Dean <me@imjoshdean.com>",
  "author": "Sheva Goldberg <b@shevagoldberg.com>",
  "author": "Mark Stahl <mjstahl@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mjstahl/es6-starter-kit/issues"
  },
  "homepage": "https://bronycon.org",
  "devDependencies": {
    "babel-cli": "^6.7.7",
    "babel-eslint": "^6.0.3",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-async-to-generator": "^6.8.0",
    "babel-plugin-transform-regenerator": "^6.14.0",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-3": "^6.11.0",
    "chai": "^3.5.0",
    "eslint": "^2.8.0",
    "eslint-config-airbnb": "^7.0.0",
    "mocha": "^2.4.5",
    "npm-run-all": "^1.7.0",
    "nyc": "^8.1.0",
    "rimraf": "^2.5.2",
    "slackbots": "^0.5.2"
  },
  "dependencies": {},
  "engines": {
    "node": "^6.0.0",
    "npm": "^3.0.0"
  }
}
